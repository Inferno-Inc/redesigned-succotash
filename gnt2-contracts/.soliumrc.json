{
  "extends": "solium:recommended",
  "plugins": [
    "security"
  ],
  "rules": {
    "quotes": [
      "error",
      "double"
    ],
    "indentation": [
      "error",
      4
    ],
    "arg-overflow": [
      "warning",
      6
    ],
    "security/no-block-members": [
      1,
      [
        "blockhash"
      ]
    ]
  }
}
